<div class="container">

    <div class="row pb-3 align-items-center">
        <div class="col-6 order-2 order-xs-2 order-lg-1 order-md-2">
            <div class="card mt-4 animate__animated animate__bounceInRight" style="width: 18rem;">
                <img src="<%=Productss.avatar %>" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-text"style="font-family:Sacramento;font-size:52px"><%=Productss.first_name%></h5>
                </div>
                <ul class="list-group list-group-flush">
                    <li class="list-group-item"><i class="fa fa-envelope"></i> <%=Productss.email%></li>
                    <li class="list-group-item"><i class="fa fa-user"></i> <%=Productss.id%></li>
                </ul>  
            </div>
        </div>
        <div class="col-md-auto order-1 order-xs-1 order-lg-2 order-md-1 text-center">
             <h4 id="emp">Employee Detail</h4>        
            </div>
    </div>
</div>



